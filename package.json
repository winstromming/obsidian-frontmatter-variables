{
	"name": "obsidian-frontmatter-variables",
	"version": "1.0.0",
	"description": "Plugin for Obsidian (https://obsidian.md) that allows for the insertion of variables into notes that become replaced by frontmatter property values in Reading view.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"test": "jest --config jest.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"frontmatter",
		"variables",
		"markdown"
	],
	"author": "winstromming <github.com/winstromming>",
	"repository": {
		"type": "git",
		"url": "https://github.com/winstromming/obsidian-frontmatter-variables.git"
	},
	"license": "CC0-1.0",
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.10",
		"@typescript-eslint/eslint-plugin": "8.35.1",
		"@typescript-eslint/parser": "8.35.1",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.25.5",
		"jest": "^30.0.3",
		"obsidian": "latest",
		"ts-jest": "^29.4.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	}
}
